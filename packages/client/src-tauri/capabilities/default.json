{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main",
    "annotation-overlay"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "shell:allow-spawn",
    "shell:allow-kill",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "etch-core",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
